{
    "name": "taiger-portal-embedded-lambda-service",
    "version": "0.1.0",
    "bin": {
        "taiger-portal-embedded-lambda-service": "bin/taiger-portal-embedded-lambda-service.js"
    },
    "scripts": {
        "clean": "rm -rf build dist cdk.out node_modules",
        "build": "tsc",
        "prettier": "prettier --write --cache .",
        "lint": "eslint lib test constants --cache --ext .ts",
        "lint:fix": "eslint lib test constants --ext .ts --fix",
        "format": "prettier --write .",
        "prebuild": "rm -rf build dist cdk.out",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "prepare": "husky",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --write",
            "eslint --fix --ext .ts",
            "git add ."
        ]
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.152",
        "@types/jest": "^29.5.12",
        "@types/node": "20.14.9",
        "@types/pg": "^8.15.4",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "aws-cdk": "^2.176.0",
        "aws-lambda": "^1.0.7",
        "esbuild": "^0.25.8",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.4.2",
        "prettier": "^3.4.2",
        "ts-jest": "^29.1.5",
        "ts-node": "^10.9.2",
        "typescript": "~5.5.3"
    },
    "dependencies": {
        "@aws-lambda-powertools/tracer": "^2.24.1",
        "aws-cdk-lib": "2.173.4",
        "constructs": "^10.0.0",
        "openai": "^5.10.2",
        "pg": "^8.16.3"
    }
}
